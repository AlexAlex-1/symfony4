{%extends 'general.html.twig'%}
{%block title%}À propos de ce project{%endblock%}
{%block body%}
<h2>Проект {{project.name}}</h2>
{%endfor%}
<table border=1 class="table">
<label>
Tickets project
</label>
<tr>
<th>ID</th>
<th>Name</th>
<th>Description</th>
</tr>
{% for key in tickets %}
<tr>
<th>{{key.id}}</th>
<th>{{key.name}}</th>
<th><a href="/projects/tickets/{{key.id}}">{{key.description}}</a></th>
</tr>
{% endfor %}
</table>
<a href="/projects/{{project.id}}/tickets/create">Create new ticket</a>
<a href="/projects">Back</a>
<a href="/projects/edit/{{project.id}}">Edit</a>
<a href="/projects/del/id={{project.id}}">Delete</a>
{%endblock%}
